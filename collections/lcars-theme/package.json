{
    "name": "lcars-theme",
    "publisher": "hayzey",
    "displayName": "LCARS Theme",
    "description": "Star Trek LCARS inspired theme.",
    "version": "0.1.2",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Themes"
    ],
    "icon": "images/icon.png",
    "license": "https://github.com/hayzey/vscode-themes/blob/main/LICENSE",
    "homepage": "https://github.com/hayzey/vscode-themes/tree/main/collections/lcars-theme",
    "repository": {
        "type": "git",
        "url": "https://github.com/hayzey/vscode-themes.git"
    },
    "bugs": {
        "url": "https://github.com/hayzey/vscode-themes/issues"
    },
    "scripts": {
        "localinstall": "cp -r . ~/.vscode/extensions/$(basename \"$(pwd)\")",
        "ext:package": "vsce package --baseContentUrl=\"https://raw.githubusercontent.com/hayzey/vscode-themes/main/collections/lcars-theme/\" --baseImagesUrl=\"https://raw.githubusercontent.com/hayzey/vscode-themes/main/collections/lcars-theme/\"",
        "ext:publish": "vsce publish --baseContentUrl=\"https://raw.githubusercontent.com/hayzey/vscode-themes/main/collections/lcars-theme/\" --baseImagesUrl=\"https://raw.githubusercontent.com/hayzey/vscode-themes/main/collections/lcars-theme/\""
    },
    "contributes": {
        "themes": [
            {
                "label": "LCARS",
                "uiTheme": "vs-dark",
                "path": "./themes/lcars-color-theme.json"
            }
        ]
    }
}
